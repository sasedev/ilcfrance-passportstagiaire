{
	"name" : "symfony/framework-standard-edition",
	"license" : "MIT",
	"type" : "project",
	"description" : "The \"Symfony Standard Edition\" distribution",
	"autoload" : {
		"psr-4" : {
			"" : "src/"
		},
		"classmap" : [
			"app/AppKernel.php",
			"app/AppCache.php"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Tests\\" : "tests/"
		}
	},
	"require" : {
		"php" : ">=5.5.9",
		"composer/composer" : "~1.0",
		"incenteev/composer-parameter-handler" : "^2.0",
		"symfony/symfony" : "3.1.*",
		"symfony/swiftmailer-bundle" : "^2.4",
		"symfony/monolog-bundle" : "^3.0",
		"symfony/polyfill-apcu" : "^1.3",
		"symfony/assetic-bundle" : "^2.8",
		"symfony/psr-http-message-bridge" : "~1.0",
		"symfony/security-acl" : "~3.0",
		"sensio/distribution-bundle" : "^5.0",
		"sensio/framework-extra-bundle" : "^3.0.2",
		"sensio/generator-bundle" : "^3.0",
		"php-console/php-console" : "~3.1",
		"zendframework/zend-diactoros" : "~1.3",
		"alcaeus/mongo-php-adapter" : "~1.0",
		"mongodb/mongodb" : "~1.0",
		"predis/predis" : "~1.1",
		"doctrine/orm" : "^2.5",
		"doctrine/doctrine-bundle" : "^1.6",
		"doctrine/doctrine-cache-bundle" : "^1.2",
		"doctrine/doctrine-fixtures-bundle" : "~2.3",
		"doctrine/mongodb-odm-bundle" : "~3.2",
		"snc/redis-bundle" : "~2.0",
		"stof/doctrine-extensions-bundle" : "~1.2",
		"jms/aop-bundle" : "~1.2|~2.0",
		"jms/di-extra-bundle" : "~1.7|~2.0",
		"jms/security-extra-bundle" : "~1.6|~2.0",
		"jms/serializer-bundle" : "~1.1|~2.0",
		"leafo/scssphp" : "~0.6|~1.0",
		"ptachoire/cssembed" : "~1.0",
		"patchwork/jsqueeze" : "~2.0",
		"kriswallsmith/spork" : "~0.3|~1.0",
		"mrclay/minify" : "~2.3|~2.0",
		"twig/extensions" : "~1.3|~2.0",
		"knplabs/knp-components" : "~1.3|~2.0",
		"knplabs/gaufrette" : "~0.2|1.0",
		"knplabs/knp-paginator-bundle" : "~2.5|~3.0",
		"knplabs/knp-menu-bundle" : "~2.1|~3.0",
		"liip/imagine-bundle" : "^1.5|~2.0",
		"gregwar/captcha-bundle" : "~2.0|~3.0",
		"liuggio/excelbundle" : "~2.1|~3.0",
		"fxp/composer-asset-plugin" : "~1.1|~2.0",
		"sasedev/composer-plugin-filecopier" : ">=1.1.0",
		"npm-asset/blueimp-canvas-to-blob" : "~3.3",
		"npm-asset/sortablejs" : "~1.4",
		"npm-asset/jszip" : "~3.1",
		"npm-asset/jquery.easing" : "~1.4",
		"bower-asset/Ionicons" : "~2.0|~3.0",
		"bower-asset/font-awesome" : "~4.7",
		"bower-asset/html5shiv" : "~3.7",
		"bower-asset/respond" : "~1.4",
		"bower-asset/jquery-1.x" : "~1.11",
		"bower-asset/jquery-2.x" : "~2.2",
		"bower-asset/jquery-3.x" : "~3.1",
		"bower-asset/jquery-migrate-1.x" : "^1.4",
		"bower-asset/jquery-migrate-3.x" : "^3.0",
		"bower-asset/fastclick" : "~1.0",
		"bower-asset/bootstrap" : "~3.3",
		"bower-asset/pdfmake" : "~0.1",
		"bower-asset/holderjs" : "~2.9",
		"bower-asset/select2" : "~4.0",
		"bower-asset/jqueryui" : "~1.12",
		"bower-asset/jquery.inputmask" : "~3.3",
		"bower-asset/moment" : "~2.11",
		"bower-asset/moment-timezone" : "~0.5",
		"bower-asset/cropper" : "~2.2",
		"bower-asset/jquery-form" : "~3.51",
		"bower-asset/pace" : "~1.0",
		"bower-asset/fullcalendar" : "~2.9",
		"bower-asset/jquery-color" : "~2.1",
		"bower-asset/jquery-slimscroll" : "~1.3",
		"bower-asset/hinclude" : "^1.0",
		"bower-asset/jquery-mousewheel" : "~3.1",
		"bower-asset/iCheck" : "~1.0",
		"bower-asset/mjolnic-bootstrap-colorpicker" : "~2.3",
		"bower-asset/bootstrap-daterangepicker" : "~2.1",
		"bower-asset/bootstrap-datepicker" : "~1.6",
		"bower-asset/bootstrap-timepicker" : "~0.5",
		"bower-asset/eonasdan-bootstrap-datetimepicker" : "~4.17",
		"bower-asset/bootstrap-duallistbox" : "~3.0",
		"bower-asset/bootstrap-fileinput" : "~4.3",
		"bower-asset/DOMPurify" : "~0.8",
		"bower-asset/seiyria-bootstrap-slider" : "~9.1",
		"bower-asset/datatables.net" : "~1.10",
		"bower-asset/datatables.net-plugins" : "~1.10",
		"bower-asset/datatables.net-bs" : "~1.10",
		"bower-asset/datatables.net-autofill" : "~2.1",
		"bower-asset/datatables.net-autofill-bs" : "~2.1",
		"bower-asset/datatables.net-buttons" : "~1.2",
		"bower-asset/datatables.net-buttons-bs" : "~1.2",
		"bower-asset/datatables.net-colreorder" : "~1.3",
		"bower-asset/datatables.net-colreorder-bs" : "~1.3",
		"bower-asset/datatables.net-fixedcolumns" : "~3.2",
		"bower-asset/datatables.net-fixedcolumns-bs" : "~3.2",
		"bower-asset/datatables.net-fixedheader" : "~3.1",
		"bower-asset/datatables.net-fixedheader-bs" : "~3.1",
		"bower-asset/datatables.net-keytable" : "~2.1",
		"bower-asset/datatables.net-keytable-bs" : "~2.1",
		"bower-asset/datatables.net-responsive" : "~2.1",
		"bower-asset/datatables.net-responsive-bs" : "~2.1",
		"bower-asset/datatables.net-scroller" : "~1.4",
		"bower-asset/datatables.net-scroller-bs" : "~1.4",
		"bower-asset/datatables.net-select" : "~1.2",
		"bower-asset/datatables.net-select-bs" : "~1.2",
		"bower-asset/tinymce" : "~4.3",
		"bower-asset/ivan-chkv.tinymce-i18n" : "~4.0"
	},
	"require-dev" : {
		"sensio/generator-bundle" : "^3.0",
		"symfony/phpunit-bridge" : "^3.0"
	},
	"scripts" : {
		"post-install-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
			"bin/console assetic:dump --no-debug"
		],
		"post-update-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
			"bin/console assetic:dump --no-debug"
		]
	},
	"config" : {
		"bin-dir" : "bin",
		"vendor-dir" : "vendor",
		"component-dir" : "vendor",
		"process-timeout" : 1200
	},
	"extra" : {
		"symfony-app-dir" : "app",
		"symfony-bin-dir" : "bin",
		"symfony-var-dir" : "var",
		"symfony-web-dir" : "web",
		"symfony-tests-dir" : "tests",
		"symfony-assets-install" : "relative",
		"incenteev-parameters" : {
			"file" : "app/config/parameters.yml",
			"keep-outdated" : "true"
		},
		"asset-optimize-with-installed-packages" : "false",
		"asset-pattern-skip-version" : "false",
		"asset-optimize-with-conjunctive" : "false",
		"asset-installer-paths" : {
			"bower-asset-library" : "web/assets/bower",
			"npm-asset-library" : "web/assets/npm"
		},
		"asset-registry-options" : {
			"npm-searchable" : "false",
			"bower-searchable" : "false"
		},
		"filescopier" : [{
				"source" : "src/Ilcfrance/ResBundle/Resources/css",
				"destination" : "web/res/css"
			}, {
				"source" : "src/Ilcfrance/ResBundle/Resources/js",
				"destination" : "web/res/js"
			}, {
				"source" : "web/assets/bower/tinymce/plugins/",
				"destination" : "web/res/js/plugins"
			}, {
				"source" : "web/assets/bower/tinymce/skins/",
				"destination" : "web/res/js/skins"
			}, {
				"source" : "web/assets/bower/tinymce/themes/",
				"destination" : "web/res/js/themes"
			}, {
				"source" : "web/assets/bower/ivan-chkv.tinymce-i18n/langs",
				"destination" : "web/res/js/langs"
			}, {
				"source" : "src/Ilcfrance/ResBundle/Resources/images",
				"destination" : "web/res/images"
			}, {
				"source" : "src/Ilcfrance/ResBundle/Resources/imgs",
				"destination" : "web/res/imgs"
			}, {
				"source" : "src/Ilcfrance/ResBundle/Resources/img",
				"destination" : "web/res/img"
			}, {
				"source" : "src/Ilcfrance/ResBundle/Resources/fonts",
				"destination" : "web/res/fonts"
			}, {
				"source" : "web/assets/bower/font-awesome/fonts",
				"destination" : "web/res/fonts"
			}, {
				"source" : "web/assets/bower/ionicons/fonts",
				"destination" : "web/res/fonts"
			}, {
				"source" : "src/Ilcfrance/ResBundle/Resources/appicons/*",
				"destination" : "web"
			}, {
				"source" : "web/assets/bower/mjolnic-bootstrap-colorpicker/dist/img/bootstrap-colorpicker/*",
				"destination" : "web/res/img/bootstrap-colorpicker"
			}, {
				"source" : "web/assets/bower/bootstrap-fileinput/img/*",
				"destination" : "web/res/img/bootstrap-fileinput"
			}, {
				"source" : "web/assets/bower/icheck/skins/flat/*.png",
				"destination" : "web/res/img/icheck/flat"
			}
		],
		"branch-alias" : {
			"dev-master" : "3.1-dev"
		}
	}
}